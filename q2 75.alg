algoritmo "semnome"

var
    mes : vetor[1..12] de caractere
    i : inteiro
inicio
mes[1] <- "Janeiro"
mes[2] <- "fevereiro"
mes[3] <- "marco"
mes[4] <- "abril"
mes[5] <- "maio"
mes[6] <- "junho"
mes[7] <- "julho "
mes[8] <- "agsot"
mes[9] <- "aetembro"
mes[10] <- "otubro"
mes[11] <- "novembro"
mes[12] <- "dezembro"
     para i <- 1 ate 12  faca
     fimpara
// Seção de Comandos 
fimalgoritmo
